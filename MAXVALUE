public class ASWAP{
    public static void main(String[] args){
        int[] arr = {1, 3, 4, 7, 9};
        System.out.println(ASWAP(arr));
    }
    static int ASWAP(int[] arr){
        int max = arr[0];
        for(int i = 1;i<arr.length;i++){
            if(arr[i]>max){
                max = arr[i];
        }
    }
    return max;

    }
}
